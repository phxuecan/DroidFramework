<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leehq.retrofit" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RealmActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LEEHQ_SAMPLE_CODE"/>
                <category android:name="android.intent.category.LEEHQ_SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity android:name="com.leehq.activity.ConstraintActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LEEHQ_SAMPLE_CODE"/>
                <category android:name="android.intent.category.LEEHQ_SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity android:name="com.leehq.sample.glide.FirstGlideActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LEEHQ_SAMPLE_CODE"/>
                <category android:name="android.intent.category.LEEHQ_SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity android:name="com.leehq.activity.LifecycleTestActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LEEHQ_SAMPLE_CODE"/>
                <category android:name="android.intent.category.LEEHQ_SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity android:name="com.leehq.sample.rxandroid.RxAndroidActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LEEHQ_SAMPLE_CODE"/>
                <category android:name="android.intent.category.LEEHQ_SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity android:name="com.leehq.sample.rxandroid.CheeseActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LEEHQ_SAMPLE_CODE"/>
                <category android:name="android.intent.category.LEEHQ_SAMPLE_CODE" />
            </intent-filter>
        </activity>
    </application>

</manifest>